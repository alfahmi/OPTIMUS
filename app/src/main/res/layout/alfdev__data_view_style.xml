<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
	android:padding="5dp">

 	<TextView
		android:id="@+id/id"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="0dp" />
 	<TextView
		android:id="@+id/alfdev__data_input_office"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="15dp" />
	
	<TextView
		android:id="@+id/alfdev__data_input_canvasser"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="15dp" />
	
	<TextView
		android:id="@+id/alfdev__data_input_day"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="15dp" />
	
	<TextView
		android:id="@+id/alfdev__data_input_outlet"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="15dp" />
	
	<TextView
		android:id="@+id/alfdev__data_input_form_survey"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="15dp" />
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:background="?attr/colorAccent">

		<TextView
			android:layout_weight="1.0"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textSize="15dp"
			android:text="TELKOSMEL"/>
		<TextView
			android:layout_weight="1.0"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textSize="15dp"
			android:text="INDOSAT"/>
		<TextView
			android:layout_weight="1.0"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textSize="15dp"
			android:text="AXIS"/>

	</LinearLayout>
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal">
		
		<TextView
			android:id="@+id/alfdev__data_input_telkomsel"
			android:layout_weight="1.0"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textSize="15dp" />
		<TextView
			android:id="@+id/alfdev__data_input_indosat"
			android:layout_weight="1.0"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textSize="15dp" />
		<TextView
			android:id="@+id/alfdev__data_input_axis"
			android:layout_weight="1.0"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textSize="15dp" />
		
	</LinearLayout>
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:background="?attr/colorAccent">

		<TextView
			android:layout_weight="1.0"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textSize="15dp"
			android:text="XL"/>
		<TextView
			android:layout_weight="1.0"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textSize="15dp"
			android:text="THREE"/>
		<TextView
			android:layout_weight="1.0"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textSize="15dp"
			android:text="OTHER"/>

	</LinearLayout>
	<LinearLayout
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:orientation="horizontal">

		<TextView
			android:id="@+id/alfdev__data_input_xl"
			android:layout_weight="1.0"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textSize="15dp" />
		<TextView
			android:id="@+id/alfdev__data_input_three"
			android:layout_weight="1.0"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textSize="15dp" />
		<TextView
			android:id="@+id/alfdev__data_input_other"
			android:layout_weight="1.0"
			android:layout_width="0dp"
			android:layout_height="wrap_content"
			android:gravity="center"
			android:textSize="15dp" />

	</LinearLayout>
	
</LinearLayout>
